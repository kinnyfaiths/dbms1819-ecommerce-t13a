<style>

body{
	background-color: #ffeecc;
}

.header{
	position: relative;
	font-family: Trebuchet MS;
  font-size: 80%;
  width: 100%;
  padding: 10px 10px;
  align-content: center;
  color: white;
  background-color: #138D75;
}

.pdlist {
	font-family: Trebuchet MS;
	color: white;
	font-size: 15px;
	float: center;
}

.button {
   display: inline-block;
  background-color: #138D75;
  border-radius: 6px;
  font-size: 15px;
  color: #FFFFFF;
  text-decoration: none;
  padding: 1px 15px;
  transition: all .5s;
}
.name{
	font-size: 30px;
}

.products {
	background-color: #138D75;
	border: 1px solid #138D75;
    text-align: left;
    width: 60%;
    text-color: white;
    padding:20px 70px;
    border-left-width: 50px;
    margin: 40px 40px;
}

.topnav {
  overflow: hidden;
  background-color: #138D75;

}

.topnav a {
  float: left;
  display: block;
  color: black;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
  font-size: 17px;
}

.topnav a:hover {
  background-color: #ddd;
  color: black;
}

.topnav a.active {
  background-color: #2196F3;
  color: white;
}

.topnav .search-container {
  float: right;
}

.topnav input[type=text] {
  padding: 6px;
  margin-top: 8px;
  font-size: 17px;
  border: none;
}

.topnav .search-container button {
  float: right;
  padding: 6px 10px;
  margin-top: 8px;
  margin-right: 16px;
  background: #ddd;
  font-size: 17px;
  border: none;
  cursor: pointer;
}

.topnav .search-container button:hover {
  background: #ccc;
}

@media screen and (max-width: 600px) {
  .topnav .search-container {
    float: none;
  }
  .topnav a, .topnav input[type=text], .topnav .search-container button {
    float: none;
    display: block;
    text-align: left;
    width: 100%;
    margin: 0;
    padding: 14px;
  }
  .topnav input[type=text] {
    border: 1px solid #ccc;  
  }
}

.form {
	border: solid 1px #99e6ff;
	background-color: #99e6ff;
	border-width: 70%;
}

.form h2{
	font-family:Trebuchet MS;
	text-align: center;
}

 .navbar-expand-sm .navbar-nav .dropdown-menu {
    position: absolute;
  }
  .navbar-expand-sm .navbar-nav .dropdown-menu-right {
    right: 0;
    left: auto;
